@model List<Category>



<h1>Category List</h1>
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Display Order</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.OrderBy(u => u.DisplayOrder))
         {
            <tr>
                <td>@item.ID</td>
                <td>@item.Name</td>
                <td>@item.DisplayOrder</td>
            
            </tr>
         }
    </tbody>
</table>
